{
  "name": "geoverview",
  "version": "1.0.2",
  "description": "Based on Maplibre, geoverview is a tool to display very easily any geojson (and the information it contains) on a map.",
  "main": "src/index.js",
  "module": "src/index.js",
  "jsdelivr": "dist/index.min.js",
  "unpkg": "dist/index.min.js",
  "exports": {
    "umd": "./dist/index.min.js",
    "default": "./src/index.js"
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "rollup --config",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neocarto/geoverview.git"
  },
  "keywords": [
    "geojson",
    "map",
    "cartogrphy",
    "summary",
    "overview",
    "maplibre"
  ],
  "author": "Nicolas Lambert",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/neocarto/geoverview/issues"
  },
  "homepage": "https://github.com/neocarto/geoverview#readme",
  "devDependencies": {
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-terser": "^7.0.2"
  },
  "dependencies": {
    "@turf/area": "^6.5.0",
    "@turf/bbox": "^6.5.0",
    "@turf/length": "^6.5.0",
    "maplibre-gl": "^2.1.9",
    "topojson-client": "^3.1.0",
    "topojson-server": "^3.0.1"
  }
}
